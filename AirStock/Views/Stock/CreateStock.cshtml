@model AirStock.Models.StockAdapter

<h2>Create Stock</h2>

<form method="post">
    <div class="form-group">
        <label for="ProductName">Product Name</label>
        <input type="text" class="form-control" id="ProductName" name="ProductName" />
    </div>
    <div class="form-group">
        <label for="ProductQuantity">Product Quantity</label>
        <input type="text" class="form-control" id="ProductQuantity" name="ProductQuantity" />
    </div>
    <div class="form-group">
        <label for="ProductDate">Product Date</label>
        <input type="text" class="form-control" id="ProductDate" name="ProductDate" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
